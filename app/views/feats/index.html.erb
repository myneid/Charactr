<h1>Listing feats</h1>

<table>
  <tr>
  </tr>

<% for feat in @feats %>
  <tr>
    <td><%= link_to 'Show', character_feat_path(@character, feat) %></td>
    <td><%= link_to 'Edit', edit_character_feat_path(@character, feat) %></td>
    <td><%= link_to 'Destroy', destroy_character_feat_path(@character, feat), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New feat', new_character_feat_path(@character) %>
