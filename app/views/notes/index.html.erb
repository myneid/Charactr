<h1>Listing notes</h1>

<table>
  <tr>
  </tr>

<% for note in @notes %>
  <tr>
    <td><%= link_to 'Show', character_note_path(@character, note) %></td>
    <td><%= link_to 'Edit', edit_character_note_path(@character, note) %></td>
    <td><%= link_to 'Destroy', destroy_character_note_path(@character, note), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New note', new_character_note_path(@character) %>
